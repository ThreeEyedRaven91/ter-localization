{
  "name": "ter-localization",
  "version": "0.7.5",
  "description": "Quick and powerful library for localization for react / react native application",
  "main": "dist/libraries/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel --presets=@babel/env,@babel/react --plugins=@babel/plugin-proposal-class-properties -d dist/libraries/ src/libraries",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ThreeEyedRaven91/ter-localization.git"
  },
  "keywords": [
    "localization"
  ],
  "author": "ThreeEyedRaven91",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThreeEyedRaven91/ter-localization/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "axios": "^0.18.0",
    "react": "^16.5.2",
    "string-template": "^1.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "ignore": [
      "data/*",
      "dist/*",
      "docs/*",
      "localization/*",
      "public/*",
      "build/*"
    ],
    "delay": "2500"
  }
}
